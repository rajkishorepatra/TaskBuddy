{
    "manifest_version": 3,
    "name": "TaskBuddy",
    "version": "1.0",
    "description": "Your task management buddy!",
    "action": {
        "default_popup": "popup.html"
    },
    "permissions": [
        "storage"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "icons": {
      "16": "todo.png",
      "48": "todo.png",
      "128": "todo.png"
    }
}